{
  "name": "halolight-cloudflare",
  "version": "0.1.0",
  "private": true,
  "description": "HaloLight 后台管理系统 Cloudflare 部署版本，基于 Next.js 15 + React 19 + Edge Runtime",
  "author": "h7ml <h7ml@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/halolight/halolight-cloudflare"
  },
  "homepage": "https://halolight-cloudflare.h7ml.cn",
  "bugs": {
    "url": "https://github.com/halolight/halolight-cloudflare/issues"
  },
  "keywords": [
    "nextjs",
    "nextjs15",
    "react19",
    "cloudflare",
    "cloudflare-workers",
    "cloudflare-pages",
    "edge-runtime",
    "admin",
    "admin-dashboard",
    "dashboard",
    "typescript",
    "tailwindcss",
    "tailwindcss4",
    "shadcn-ui",
    "opennext",
    "halolight"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:cf": "opennextjs-cloudflare build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "pages:build": "opennextjs-cloudflare build",
    "pages:deploy": "wrangler pages deploy .open-next --project-name=halolight-cloudflare"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@opennextjs/cloudflare": "^1.11.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.11",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.23.24",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.554.0",
    "mockjs": "^1.1.0",
    "next": "15.5.7",
    "next-themes": "^0.4.6",
    "react": "19.1.2",
    "react-dom": "19.1.2",
    "react-grid-layout": "^1.5.2",
    "react-hook-form": "^7.66.1",
    "recharts": "^3.5.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/js-cookie": "^3.0.6",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-grid-layout": "^1.3.6",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.14",
    "eslint": "^9",
    "eslint-config-next": "15.5.7",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jsdom": "^27.2.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.14",
    "wrangler": "^4.51.0"
  },
  "packageManager": "pnpm@10.23.0"
}
